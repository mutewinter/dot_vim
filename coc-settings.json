{
  "codeLens.enable": true,
  "javascript.suggestionActions.enabled": false,
  "javascript.referencesCodeLens.enable": false,
  "typescript.referencesCodeLens.enable": false,
  "typescript.implementationsCodeLens.enable": false,
  "suggest.triggerAfterInsertEnter": true,
  "suggest.noselect": false,
  "suggest.labelMaxLength": 70,
  "diagnostic.errorSign": "✖",
  "diagnostic.infoSign": "ⓘ",
  "prettier.onlyUseLocalVersion": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "graphql",
    "typescript",
    "typescriptreact"
  ],
  "cSpell.userWords": [
    "Postgres",
    "browserslist",
    "cocstatus",
    "codegen",
    "concat",
    "graphql",
    "jsonb",
    "mutewinter",
    "pbcopy",
    "smallint",
    "unoptimized",
    "uuid"
  ],
  "coc.preferences.rootPatterns": [
    ".git",
    ".hg",
    ".projections.json",
    "tsconfig.json"
  ],
  "cSpell.enabledLanguageIds": [
    "vim",
    "asciidoc",
    "c",
    "cpp",
    "csharp",
    "css",
    "git-commit",
    "go",
    "handlebars",
    "haskell",
    "html",
    "jade",
    "java",
    "javascript",
    "javascriptreact",
    "json",
    "jsonc",
    "latex",
    "less",
    "markdown",
    "php",
    "plaintext",
    "python",
    "pug",
    "restructuredtext",
    "rust",
    "scala",
    "scss",
    "text",
    "typescript",
    "typescriptreact",
    "yaml",
    "yml",
    "sql"
  ],
  "tailwindCSS.headwind.runOnSave": false,
  "tailwindCSS.colorDecorators": "on",
  "tailwindCSS.experimental.classRegex": [
    "tw`([^`]*)",
    "tw=\"([^\"]*)",
    "tw={\"([^\"}]*)",
    "tw\\.\\w+`([^`]*)",
    "tw\\(.*?\\)`([^`]*)"
  ],
  "snippets.userSnippetsDirectory": "~/.config/nvim/MyUltiSnips"
}
